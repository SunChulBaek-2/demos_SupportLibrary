<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
									  xmlns:app="http://schemas.android.com/apk/res-auto"
									  android:layout_width="match_parent"
									  android:layout_height="match_parent"
									  app:columnCount="2"
									  app:orientation="horizontal"
									  android:clickable="true"
									  android:background="#ffffff">
	<!-- row 0 -->
	<TextView
		app:layout_row="0"
		app:layout_column="0"
		android:text="TextView" />

	<android.support.v7.widget.AppCompatTextView
		app:layout_row="0"
		app:layout_column="1"
		android:text="AppCompat" />

	<!-- row 1 -->
	<AutoCompleteTextView
		app:layout_row="1"
		app:layout_column="0"
		android:text="AutoCompleteTextView"/>

	<android.support.v7.widget.AppCompatAutoCompleteTextView
		app:layout_row="1"
		app:layout_column="1"
		android:text="AppCompatAutoCompleteTextView"/>

	<!-- row 2 -->
	<Button
		app:layout_row="2"
		app:layout_column="0"
		android:text="Button"/>

	<android.support.v7.widget.AppCompatButton
		app:layout_row="2"
		app:layout_column="1"
		android:text="AppCompatButton"/>

	<!-- row 3 -->
	<CheckBox
		app:layout_row="3"
		app:layout_column="0"
		android:text="CheckBox"/>

	<android.support.v7.widget.AppCompatCheckBox
		app:layout_row="3"
		app:layout_column="1"
		android:text="AppCompatCheckBox"/>

	<!-- row 4 -->
	<CheckedTextView
		app:layout_row="4"
		app:layout_column="0"
		android:text="CheckedTextView"/>

	<android.support.v7.widget.AppCompatCheckedTextView
		app:layout_row="4"
		app:layout_column="1"
		android:text="AppCompatCheckedTextView"/>

	<!-- row 5 -->
	<EditText
		app:layout_row="5"
		app:layout_column="0"
		android:text="EditText"/>

	<android.support.v7.widget.AppCompatEditText
		app:layout_row="5"
		app:layout_column="1"
		android:text="AppCompatEditText"/>

	<!-- row 6 -->
	<MultiAutoCompleteTextView
		app:layout_row="6"
		app:layout_column="0"
		android:text="MultiAutoCompleteTextView"/>

	<android.support.v7.widget.AppCompatMultiAutoCompleteTextView
		app:layout_row="6"
		app:layout_column="1"
		android:text="AppCompatMultiAutoCompleteTextView"/>

	<!-- row 7 -->
	<RadioButton
		app:layout_row="7"
		app:layout_column="0"
		android:text="RadioButton"/>

	<android.support.v7.widget.AppCompatRadioButton
		app:layout_row="7"
		app:layout_column="1"
		android:text="AppCompatRadioButton"/>

	<!-- row 8 -->
	<RatingBar
		app:layout_row="8"
		app:layout_column="0"
		android:text="RatingBar"/>

	<android.support.v7.widget.AppCompatRatingBar
		app:layout_row="8"
		app:layout_column="1"
		android:text="AppCompatRatingBar"/>

	<!-- row 9 -->
	<Spinner
		app:layout_row="9"
		app:layout_column="0"
		android:text="Spinner"/>

	<android.support.v7.widget.AppCompatSpinner
		app:layout_row="9"
		app:layout_column="1"
		android:text="AppCompatSpinner"/>
</android.support.v7.widget.GridLayout>